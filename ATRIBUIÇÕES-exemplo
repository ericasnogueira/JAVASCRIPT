//  =  x = y   x = y
// +=  x += y  x = x + y
// -=  x -= y  x = x - y
// *=  x *= y  x = x * y
// /=  x /=y   x = x / y
// %=  x %= y  x = x % y  


// varias maneiras de fazer a mesma coisa 
//exemplos

//soma
var a=30;
var b = 30;
var c = 30;

a++; //+1
b=b+1; 
c += 1;
console.log(a);
console.log(b);
console.log(c);

//divis√£o
var d = 20;
var e =20;
d = d / 10;
e /= 10;
console.log(d);
console.log(e);
















