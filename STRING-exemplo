

var src = "ira desse modo"
var er = 'desse tambem'
var e = 'assim "teste" tambem vai '
var ee= "olha ai 'desse jeito' tambem vai"

console.log(src)
console.log(er)
console.log(e)
console.log(ee)

//soma com strings
var s = 'olha novamente '
s +='outro'
console.log(s)
var aa = 'assim '+ 'vai '+'outo'
console.log(aa)

// com numero sรณ ira com + 
// so que ele se transforma em uma string
var eeee = 8;
var asdad = 'com numero ' + eeee;
console.log(asdad)
console.log(eeee);

//tranformando uma string em um
//numero 
var d = "5" * 6
// ele se transforma com *  e com 
// qualquer numero
var asde = "26" * 4

console.log(d);
console.log(asde)
